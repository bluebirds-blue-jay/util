{
  "name": "@bluejay/utils",
  "description": "General purpose utilities for bluejay modules.",
  "version": "4.0.0-alpha.7",
  "author": "Sylvain Estevez",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dts/index.d.ts",
  "scripts": {
    "docs": "./node_modules/.bin/typedoc --out docs --mode file --excludePrivate --excludeExternals ./index.ts",
    "test": "mocha",
    "cov": "nyc mocha",
    "lint": "tslint --project ./",
    "fix": "tslint --fix --project ./",
    "compile": "tsc",
    "build": "rm -rf dist && mkdir dist && tsc --noEmit false",
    "prepare": "tsc",
    "prepublishOnly": "npm run lint && npm run cov && npm run build",
    "travis": "tsc && npm run lint && npm run cov"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluebirds-blue-jay/utils.git"
  },
  "keywords": [
    "bluejay",
    "utils"
  ],
  "bugs": {
    "url": "https://github.com/bluebirds-blue-jay/utils/issues"
  },
  "homepage": "https://github.com/bluebirds-blue-jay/utils#readme",
  "dependencies": {
    "@bluejay/collection": "5.0.0-alpha.8",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "5.2.3",
    "@types/node": "12.7.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "15.1.0",
    "ts-node": "^9.0.0",
    "tslint": "6.1.3",
    "typedoc": "0.15.0",
    "typescript": "^4.0.2"
  }
}
